<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="615.0" prefWidth="893.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gestion.utilisateur.AjouterPanierController">
   <children>
      <Text layoutX="280.0" layoutY="93.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Ajouter Panier">
         <font>
            <Font size="48.0" />
         </font>
      </Text>
      <Text layoutX="304.0" layoutY="263.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Nom de produit" />
      <TextField fx:id="nomProduit" layoutX="304.0" layoutY="288.0" promptText="entrez le nom du produit" />
      <Text layoutX="304.0" layoutY="357.0" strokeType="OUTSIDE" strokeWidth="0.0" />
      <Button fx:id="btn_ajouter" layoutX="304.0" layoutY="534.0" mnemonicParsing="false" onAction="#ajouter" text="Ajouter" />
      <Button fx:id="btn_vider" layoutX="454.0" layoutY="534.0" mnemonicParsing="false" onAction="#reset" text="Vider" />
      <ImageView fx:id="img" fitHeight="121.0" fitWidth="164.0" layoutX="304.0" layoutY="114.0" onMouseClicked="#charger_image" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="btn_img" layoutX="549.0" layoutY="534.0" mnemonicParsing="false" onAction="#charger_image" text="choisir une image" />
      <ComboBox fx:id="comboProduit" layoutX="304.0" layoutY="210.0" onMouseClicked="#charge" prefWidth="150.0" />
   </children>
</AnchorPane>
